{
    "name": "dimafe6/bank-id-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for Swedish BankID library",
    "version": "1.0.0",
    "keywords": [
        "dimafe6",
        "bank-id-bundle",
        "symfony",
        "bank-id"
    ],
    "homepage": "https://github.com/dimafe6/BankIDBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dimafe2000@gmail.com"
        }

    ],
    "require": {
        "php" : ">=5.6.0",
        "symfony/framework-bundle": "^2.7|^3.0",
        "dimafe6/bank-id": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.7.* || 6.3.*"
    },
    "autoload": {
        "psr-4": {
            "Dimafe6\\BankIDBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BankIDBundle\\Test\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
